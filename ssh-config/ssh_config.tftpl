# === ssh_hosts settings ===
%{ for key, server in servers ~}
Host ${server.name}
  Hostname ${server.floating_ip}
  User ${server.ssh_user}
  IdentityFile ${server.ssh_key_path}
%{ endfor ~}

# === all host settings ===
Host *
  ServerAliveInterval 15
  ServerAliveCountMax 10
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
